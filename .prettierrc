{
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 4,
    "useTabs": false,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "overrides": [
        {
            "files": "*.{ts,tsx,js,jsx,json,md,html,css,scss}",
            "options": {
                "tabWidth": 4
            }
        }
    ]
}
